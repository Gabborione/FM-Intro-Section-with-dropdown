\n    width: 100%;\r\n    height: 100vh;\r\n    z-index: 0;\r\n    margin: 0;\r\n}\r\n\r\n.overlay::after {\r\n    content: \"\";\r\n    display: block;\r\n    position: fixed;\r\n    padding: 100px 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    top: 0;\r\n    left: 0;\r\n    background-color: rgba(black, 0.6);\r\n}\r\n","sass\\components\\_navbar.scss":"@use \"../abstracts/colors\" as *;\r\n\r\n$bp-mobile: 820px;\r\n\r\n.hidden {\r\n    display: none;\r\n}\r\n\r\n.header {\r\n    top: 0;\r\n    right: 0;\r\n    width: 100vw;\r\n    height: 60px;\r\n    position: fixed;\r\n    z-index: 99;\r\n    padding: 35px 25px;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n}\r\n\r\n.header__logo {\r\n    position: relative;\r\n    top: 0;\r\n    left: 0;\r\n    font-size: 1rem;\r\n    font-weight: 700;\r\n    color: #000;\r\n    text-decoration: none;\r\n    cursor: pointer;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: flex-start;\r\n}\r\n\r\n.header__hamburger {\r\n    position: relative;\r\n    display: block;\r\n    top: 0;\r\n    right: 0;\r\n    cursor: pointer;\r\n    z-index: 15;\r\n\r\n    @media screen and (min-width: $bp-mobile) {\r\n        display: none;\r\n    }\r\n}\r\n\r\nnav {\r\n    position: absolute;\r\n    top: 0;\r\n    right: 0;\r\n    width: 100%;\r\n    height: 100vh;\r\n    z-index: 10;\r\n\r\n    @media screen and (min-width: $bp-mobile) {\r\n        position: relative;\r\n        width: 100%;\r\n        height: 60px;\r\n    }\r\n}\r\n\r\n.header__nav {\r\n    position: absolute;\r\n    right: 0;\r\n    top: 0;\r\n    height: 100%;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: flex-start;\r\n    align-items: flex-start;\r\n    inset: 0 0 0 35%;\r\n    padding: 4em 1.5em;\r\n    background-color: $almost-white;\r\n    gap: 2em;\r\n\r\n    @media screen and (min-width: $bp-mobile) {\r\n        position: relative;\r\n        padding: 0;\r\n        background-color: transparent;\r\n        height: 60px;\r\n        flex-direction: row;\r\n        align-items: center;\r\n        justify-content: space-between;\r\n        top: 0;\r\n        left: 0;\r\n    }\r\n}\r\n\r\n.header__list {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: flex-start;\r\n    align-items: flex-start;\r\n    margin-top: 1em;\r\n    gap: 1.5em;\r\n\r\n    @media screen and (min-width: $bp-mobile) {\r\n        margin-left: 3em;\r\n        width: 100%;\r\n        padding: 0;\r\n        background-color: transparent;\r\n        height: 60px;\r\n        flex-direction: row;\r\n        align-items: center;\r\n        justify-content: flex-start;\r\n        inset: none;\r\n        left: 0;\r\n        margin-top: 0;\r\n    }\r\n}\r\n\r\n.header__nav-item {\r\n    display: flex;\r\n    justify-content: flex-start;\r\n    align-items: center;\r\n    gap: 0.8em;\r\n}\r\n\r\n.header__nav-link {\r\n    font-size: 1em;\r\n    color: $medium-gray;\r\n    margin: 0;\r\n}\r\n\r\n.arrow-down {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n\r\n.login-container {\r\n    width: 100%;\r\n    display: flex;\r\n    justify-content: center;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    gap: 0.8rem;\r\n\r\n    @media screen and (min-width: $bp-mobile) {\r\n        flex-direction: row;\r\n        justify-content: flex-end;\r\n        align-items: center;\r\n        margin: 0;\r\n        gap: 2rem;\r\n    }\r\n}\r\n\r\n.login {\r\n    width: 100%;\r\n    text-align: center;\r\n    font-size: 0.9em;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n\r\n    @media screen and (min-width: $bp-mobile) {\r\n        width: auto;\r\n    }\r\n}\r\n\r\n.register {\r\n    width: 100%;\r\n    padding-top: 0.6em;\r\n    padding-bottom: 0.6em;\r\n    border-radius: 15px;\r\n    border: 2px solid $medium-gray;\r\n    font-size: 0.9em;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n\r\n    @media screen and (min-width: $bp-mobile) {\r\n        width: 6rem;\r\n    }\r\n}\r\n","sass\\abstracts\\_colors.scss":"$almost-white: hsl(0, 0%, 98%);\r\n$medium-gray: hsl(0, 0%, 41%);\r\n$almost-black: hsl(0, 0%, 8%);","sass\\components\\_hero.scss":"@use \"../abstracts/colors\" as *;\r\n\r\n.hero {\r\n    margin-top: 80px;\r\n    width: 100%;\r\n    z-index: 0;\r\n}\r\n\r\n.hero__img-container {\r\n    width: 100%;\r\n    height: 100%;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    z-index: -1;\r\n\r\n    img {\r\n        width: 100%;\r\n        height: 100%;\r\n    }\r\n}\r\n\r\n.hero__text {\r\n    padding: 25px;\r\n    margin-top: 1rem;\r\n    width: 100%;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: flex-start;\r\n    align-items: center;\r\n    gap: 1rem;\r\n}\r\n\r\n.hero__title {\r\n    font-size: 1.89rem;\r\n    font-weight: 700;\r\n    color: $almost-black;\r\n}\r\n\r\n.hero__subtitle {\r\n    line-height: 1.2rem;\r\n    font-size: 0.85rem;\r\n    font-weight: 500;\r\n    color: $medium-gray;\r\n    text-align: center;\r\n}\r\n\r\n.hero__btn {\r\n    background-color: $almost-black;\r\n    color: $almost-white;\r\n    font-size: 0.8rem;\r\n    font-weight: 700;\r\n    padding: 1em 1.5em;\r\n    border-radius: 15px;\r\n    cursor: pointer;\r\n}\r\n\r\n.hero__sponsor-container {\r\n    padding: 20px;\r\n    max-width: 100%;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    gap: 1rem;\r\n\r\n    img {\r\n        width: 100%;\r\n        flex: 1 1 25%;\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"6b245fc8936988b2d1ff4cfedcf06830","cacheData":{"env":{}}}